{
  "name": "vue-startup",
  "version": "0.0.1",
  "description": "A project template with vue",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "clean": "node ./build/clean.js",
    "dev": "cross-env BUILD_ENV=development webpack-dev-server --progress --config ./build/webpack.config.dev.js",
    "build:dev": "npm run clean && cross-env BUILD_ENV=development webpack --display-optimization-bailout --config ./build/webpack.config.dist.js",
    "build:test": "npm run clean && cross-env BUILD_ENV=test webpack --display-optimization-bailout --config ./build/webpack.config.dist.js",
    "build:pre": "npm run clean && cross-env BUILD_ENV=pre-production webpack --display-optimization-bailout --config ./build/webpack.config.dist.js",
    "build:prod": "npm run clean && cross-env BUILD_ENV=production webpack --display-optimization-bailout --config ./build/webpack.config.dist.js",
    "lint": "eslint --ext .js,.vue src build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KyLeoHC/vue-startup.git"
  },
  "author": "KyLeo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KyLeoHC/vue-startup/issues"
  },
  "homepage": "https://github.com/KyLeoHC/vue-startup#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@babel/runtime": "^7.3.1",
    "@babel/runtime-corejs2": "^7.3.1",
    "autoprefixer": "^9.4.7",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-import": "^1.11.0",
    "commander": "^2.19.0",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css": "^2.2.3",
    "css-loader": "^2.1.0",
    "eslint": "^5.13.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.1",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^3.2.0",
    "inline-source-webpack-plugin": "^1.0.1",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-url": "^8.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "vant": "^1.5.1",
    "vconsole": "^3.3.0",
    "vue": "^2.6.6",
    "vue-loader": "^15.6.2",
    "vue-router": "^3.0.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.6",
    "webpack": "^4.29.3",
    "webpack-bundle-analyzer": "^3.0.4",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14"
  },
  "browserslist": [
    "Android >= 4.0",
    "iOS >= 7"
  ]
}
