{
  "name": "vue-startup",
  "version": "0.0.1",
  "description": "A project template with vue",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "precommit": "lint-staged",
    "postcommit": "git update-index --again",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "clean": "node ./build/clean.js",
    "dev": "cross-env BUILD_ENV=development webpack-dev-server --progress --config ./build/webpack.config.dev.js",
    "build:dev": "npm run clean && cross-env BUILD_ENV=development webpack --config ./build/webpack.config.dist.js",
    "build:test": "npm run clean && cross-env BUILD_ENV=test webpack --config ./build/webpack.config.dist.js",
    "build:pre": "npm run clean && cross-env BUILD_ENV=pre-production webpack --config ./build/webpack.config.dist.js",
    "build:prod": "npm run clean && cross-env BUILD_ENV=production webpack --config ./build/webpack.config.dist.js",
    "lint": "eslint --ext .js,.vue,.ts src",
    "stylelint": "stylelint 'src/**/*.{vue,htm,html,css,sss,less,scss,sass}'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KyLeoHC/vue-startup.git"
  },
  "author": "KyLeo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KyLeoHC/vue-startup/issues"
  },
  "homepage": "https://github.com/KyLeoHC/vue-startup#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/polyfill": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/runtime": "^7.4.2",
    "@babel/runtime-corejs2": "^7.4.2",
    "@typescript-eslint/eslint-plugin": "^1.5.0",
    "@typescript-eslint/parser": "^1.5.0",
    "autoprefixer": "^9.5.0",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-import": "^1.11.0",
    "commander": "^2.19.0",
    "copy-webpack-plugin": "^5.0.2",
    "core-js": "^3.0.0",
    "cross-env": "^5.2.0",
    "css": "^2.2.3",
    "css-loader": "^2.1.1",
    "eslint": "^5.15.3",
    "eslint-config-standard": "^12.0.0",
    "eslint-formatter-friendly": "^6.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "inline-source-webpack-plugin": "^1.2.0",
    "js-cookie": "^2.2.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "lint-staged": "^8.1.5",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-url": "^8.0.0",
    "sass-loader": "^7.1.0",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-scss": "^3.5.4",
    "stylelint-webpack-plugin": "^0.10.5",
    "typescript": "^3.3.4000",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "vant": "^1.6.11",
    "vconsole": "^3.3.0",
    "vue": "^2.6.10",
    "vue-loader": "^15.6.4",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.0.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "vuex": "^3.1.0",
    "webpack": "^4.29.5",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.0"
  },
  "browserslist": [
    "Android >= 4.0",
    "iOS >= 7"
  ],
  "husky": {
    "hooks": {
      "precommit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "npm run lint",
      "git add"
    ],
    "src/**/*.{vue,htm,html,css,sss,less,scss,sass}": [
      "npm run stylelint",
      "git add"
    ]
  }
}
