{
  "name": "vue-startup",
  "version": "0.0.1",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "clean": "node ./build/clean.js",
    "dev": "cross-env BUILD_ENV=development webpack-dev-server --progress --config ./build/webpack.config.dev.js",
    "build:dev": "npm run clean && npm run type-check && cross-env BUILD_ENV=development webpack --config ./build/webpack.config.dist.js",
    "build:test": "npm run clean && npm run type-check && cross-env BUILD_ENV=test webpack --config ./build/webpack.config.dist.js",
    "build:pre": "npm run clean && npm run type-check && cross-env BUILD_ENV=pre-production webpack --config ./build/webpack.config.dist.js",
    "build:prod": "npm run clean && npm run type-check && cross-env BUILD_ENV=production webpack --config ./build/webpack.config.dist.js",
    "lint": "eslint --ext .js,.vue,.ts src tests",
    "stylelint": "stylelint 'src/**/*.{vue,htm,html,css,sss,less,scss,sass}'",
    "test:unit": "jest"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/runtime": "^7.5.5",
    "@types/jest": "^24.0.15",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "autoprefixer": "^9.6.1",
    "axios": "^0.19.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.12.0",
    "copy-webpack-plugin": "^5.0.3",
    "core-js": "^3.1.4",
    "cross-env": "^5.2.0",
    "css": "^2.2.3",
    "css-loader": "^3.1.0",
    "eslint": "^6.1.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-formatter-friendly": "^7.0.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.3",
    "glob": "^7.1.4",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.1",
    "inline-source-webpack-plugin": "^1.3.0",
    "jest": "^24.8.0",
    "jest-serializer-vue": "^2.0.2",
    "jest-watch-typeahead": "^0.3.1",
    "js-cookie": "^2.2.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^9.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "postcss-url": "^8.0.0",
    "sass-loader": "^7.1.0",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-scss": "^3.9.2",
    "stylelint-webpack-plugin": "^0.10.5",
    "typescript": "^3.5.3",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "vant": "^2.0.9",
    "vconsole": "^3.3.2",
    "vue": "^2.6.10",
    "vue-jest": "^3.0.4",
    "vue-loader": "^15.7.1",
    "vue-property-decorator": "^8.2.1",
    "vue-router": "^3.0.7",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "vuex": "^3.1.1",
    "vuex-class": "^0.3.2",
    "webpack": "^4.37.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  },
  "browserslist": [
    "Android >= 4.0",
    "iOS >= 7"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,vue}": [
      "npm run lint",
      "git add"
    ],
    "src/**/*.{vue,htm,html,css,sss,less,scss,sass}": [
      "npm run stylelint",
      "git add"
    ]
  }
}
